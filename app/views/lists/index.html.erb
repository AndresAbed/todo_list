<ol>
  <% @lists.each do |list| %>
    <li><%= link_to list.name, list_path(list) %></li>
  <% end %>
</ol>

<% if flash[:notice] %>
  <p><%= flash[:notice]%></p>
<% end %>
<% if flash[:error] %>
  <p><%= flash[:error]%></p>
<% end %>


<%= form_with model: @list, local: true do |form| %>
  <%= form.text_field :name %>
  <%= form.submit %>
<% end %>
